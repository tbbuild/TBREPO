<addons>
<addon id="plugin.program.CSaintsWizard" name="[COLOR red]Cosmic Saints[/COLOR] [COLOR ghostwhite]Wizard[/COLOR]" version="1.2.4" provider-name="Cosmic Saints">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Cosmic Saints Wizard</summary>
    <description lang="en">Cosmic Saints Wizard will allow you to install our builds, Android APK's, perform maintenance and much more.</description>
    <platform>all</platform>
	<news></news>
  </extension>
</addon>

